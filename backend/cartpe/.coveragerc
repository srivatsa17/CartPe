[run]
    branch = True
    source = .
    data_file = coverage/coverage_binary
    omit =
        *apps.py
        *manage.py
        *__init__.py
        *migrations*
        *asgi*
        *wsgi*
        *admin.py
        *urls.py
        *settings*

[report]
    omit =
        *apps.py
        *manage.py
        *__init__.py
        */migrations/*
        *asgi*
        *wsgi*
        *admin.py
        *urls.py
        *settings*

fail_under = 98